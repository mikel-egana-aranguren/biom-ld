        </rdfs:range>
    </owl:DatatypeProperty>
    


    <!-- http://www.biom-format.org/ontology/id -->

    <owl:DatatypeProperty rdf:about="&ontology;id">
        <biom-format-original-description xml:lang="en">&lt;string or null&gt; a field that can be used to id a table (or null)</biom-format-original-description>
        <rdfs:comment xml:lang="en">This has been added for the sake of completeness, since the id in BIOM-LD should be the (HTTP resolvable) URI of the biom table instance, i.e. the &quot;@id&quot; property in JSON-LD</rdfs:comment>
        <rdfs:domain rdf:resource="&ontology;Table"/>
        <rdfs:range>
            <rdfs:Datatype>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="&xsd;anyURI"/>
                    <rdf:Description rdf:about="&xsd;string"/>
                </owl:unionOf>
            </rdfs:Datatype>
        </rdfs:range>
    </owl:DatatypeProperty>
    


    <!-- http://www.biom-format.org/ontology/matrix_element_type -->

    <owl:DatatypeProperty rdf:about="&ontology;matrix_element_type">
        <biom-format-original-description xml:lang="en">matrix_element_type : Value type in matrix (a controlled vocabulary)
                      Acceptable values:
                       &quot;int&quot; : integer
                       &quot;float&quot; : floating point
                       &quot;unicode&quot; : unicode string</biom-format-original-description>
        <rdfs:domain rdf:resource="&ontology;Table"/>
        <rdfs:range>
            <rdfs:Datatype>
                <owl:onDatatype rdf:resource="&xsd;string"/>
                <owl:withRestrictions rdf:parseType="Collection">
                    <rdf:Description>
                        <xsd:pattern>int|float|unicode</xsd:pattern>
                    </rdf:Description>
                </owl:withRestrictions>
            </rdfs:Datatype>
        </rdfs:range>
    </owl:DatatypeProperty>
    


    <!-- http://www.biom-format.org/ontology/matrix_type -->

    <owl:DatatypeProperty rdf:about="&ontology;matrix_type">
        <biom-format-original-description xml:lang="en">&lt;string&gt; Type of matrix data representation (a controlled vocabulary)
                      Acceptable values:
                       &quot;sparse&quot; : only non-zero values are specified
                       &quot;dense&quot; : every element must be specified</biom-format-original-description>
        <rdfs:domain rdf:resource="&ontology;Table"/>
        <rdfs:range>
            <rdfs:Datatype>
                <owl:onDatatype rdf:resource="&xsd;string"/>
                <owl:withRestrictions rdf:parseType="Collection">
                    <rdf:Description>
                        <xsd:pattern>sparse|dense</xsd:pattern>
                    </rdf:Description>
                </owl:withRestrictions>
            </rdfs:Datatype>
        </rdfs:range>
    </owl:DatatypeProperty>
    


