    <!-- http://www.biom-format.org/ontology/Table -->

    <owl:Class rdf:about="&ontology;Table">
        <owl:equivalentClass>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="&ontology;FunctionTable"/>
                    <rdf:Description rdf:about="&ontology;GeneTable"/>
                    <rdf:Description rdf:about="&ontology;MetaboliteTable"/>
                    <rdf:Description rdf:about="&ontology;OTUTable"/>
                    <rdf:Description rdf:about="&ontology;OrthologTable"/>
                    <rdf:Description rdf:about="&ontology;PathwayTable"/>
                    <rdf:Description rdf:about="&ontology;TaxonTable"/>
                </owl:unionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:comment xml:lang="en">The BIOM table is a member of one of these classses</rdfs:comment>
    </owl:Class>
    


