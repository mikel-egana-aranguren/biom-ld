        <rdfs:domain rdf:resource="&ontology;Table"/>
        <rdfs:range rdf:resource="&xsd;string"/>
    </owl:DatatypeProperty>
    


    <!-- http://www.biom-format.org/ontology/has_value -->

    <owl:DatatypeProperty rdf:about="&ontology;has_value">
        <rdfs:range>
            <rdfs:Datatype>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="&xsd;float"/>
